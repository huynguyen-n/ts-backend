{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "import/no-mutable-exports": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "prefer-destructuring": 0,
    "camelcase": 0,
    "no-param-reassign": 0,
    "func-names": "off"
  }
}